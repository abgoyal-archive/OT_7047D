ifeq ($(PROJECT),yaris_xl)
EXTRA_CFLAGS += -DYARISXL
EXTRA_CFLAGS += -DBATT_ID_CHECK_SUPPORT
EXTRA_CFLAGS += -DYARISXL_CHARGING_TEMPERATURE_PLOICY
EXTRA_CFLAGS += -DYARIS_XL_WEAK_CHARGER
EXTRA_CFLAGS += -DYARIS_XL_THREAD_INIT_FLAG
EXTRA_CFLAGS += -DJRD_FUEL_GAUGE_OPTIMIZE
endif

ifeq ($(PROJECT),yaris_xl_td)
EXTRA_CFLAGS += -DYARISXLTD
EXTRA_CFLAGS += -DBATT_ID_CHECK_SUPPORT
EXTRA_CFLAGS += -DYARISXLTD_CHARGING_TEMPERATURE_PLOICY
EXTRA_CFLAGS += -DYARIS_XL_WEAK_CHARGER
EXTRA_CFLAGS += -DJRD_FUEL_GAUGE_OPTIMIZE
endif

ifeq ($(PROJECT),yaris_xxl)
EXTRA_CFLAGS += -DYARISXXL
EXTRA_CFLAGS += -DBATT_ID_CHECK_SUPPORT
EXTRA_CFLAGS += -DYARISXXL_CHARGING_TEMPERATURE_PLOICY
EXTRA_CFLAGS += -DYARIS_XXL_WEAK_CHARGER
endif

ifeq ($(PROJECT),yaris_xxl_td)
EXTRA_CFLAGS += -DYARIS_XXL_WEAK_CHARGER
endif

ifeq ($(PROJECT),eclipse)
EXTRA_CFLAGS += -DECLIPSE
EXTRA_CFLAGS += -DBATT_ID_CHECK_SUPPORT
EXTRA_CFLAGS += -DECLIPSE_CHARGING_TEMPERATURE_PLOICY
EXTRA_CFLAGS += -DYARIS_XXL_WEAK_CHARGER
endif

ifeq ($(TARGET_BUILD_MMITEST),true)
EXTRA_CFLAGS += -DCHARGING_META
EXTRA_CFLAGS += -DBATTERY_ID_CHECK_BOOT
EXTRA_CFLAGS += -DBATTERY_PERCENT_90
endif

obj-$(CONFIG_MTK_SMART_BATTERY) += battery_meter.o battery_common.o

ifeq ($(MTK_FAN5405_SUPPORT),yes)
    obj-$(CONFIG_MTK_SMART_BATTERY) += switch_charging.o 
else
    ifeq ($(MTK_SWCHR_SUPPORT),yes)
        obj-$(CONFIG_MTK_SMART_BATTERY) += switch_charging.o
    else
        obj-$(CONFIG_MTK_SMART_BATTERY) += linear_charging.o
    endif    
endif
